<?xml version="1.0" encoding="UTF-8"?>
<schemalist>
    <schema path="/com/krc/emu8086app/" id="com.krc.emu8086app">
        <key name="font" type="s">
            <default>'Monospace 12'</default>
            <summary>Font</summary>
            <description>The font to be used for content.</description>
        </key>
        <key name="highlight" type="b">

            <default>true</default>
            <summary>Highlight current _line</summary>
            <description>If current line should be highlighted</description>
        </key>


        <key name="ul" type="b">

            <default>true</default>
            <summary>Use license file</summary>
            <description>If license file text should be added for new files</description>
        </key>

        <key name="lf" type="s">
            <default>'none'</default>
            <summary>License File</summary>
            <description>The license file to be used for content.</description>
        </key>

        <key name="theme" type="s">
            <default>'dark+'</default>
            <summary>Theme</summary>
            <description>The editor theme </description>
        </key>
    </schema>

</schemalist>